<annotation>
    <folder>{{ folder }}</folder>
    <filename>{{ filename }}</filename>
    <path>{{ path }}</path>
    <source>
        <database>{{ database }}</database>
    </source>
    <size>
        <width>{{ width }}</width>
        <height>{{ height }}</height>
        <depth>{{ depth }}</depth>
    </size>
    <segmented>{{ segmented }}</segmented>
    <object>
        <name>{{ name }}</name>
        <pose>{{ pose }}</pose>
        <truncated>{{ truncated }}</truncated>
        <difficult>{{ difficult }}</difficult>
        <bndbox>
            <xmin>{{ xmin }}</xmin>
            <ymin>{{ ymin }}</ymin>
            <xmax>{{ xmax }}</xmax>
            <ymax>{{ ymax }}</ymax>
        </bndbox>
    </object>
</annotation>